set number
set numberwidth=1
set clipboard=unnamed
syntax on
set showcmd
set ruler
set encoding=utf-8
set showmatch
set sw=2
set tabstop=2 softtabstop=2
set shiftwidth=2

let mapleader=" "

"so ~/.vim/plugins.vim
"so ~/.vim/plugin-config.vim
so ~/.vim/maps.vim

"colorscheme gruvbox
